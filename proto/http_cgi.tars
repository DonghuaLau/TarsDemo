module HttpApp
{
	struct st_cgi_req
	{
		0 require int mode; // 0: GET, 1: POST
		1 require map<string, string> query;
		2 require string cookie;
	};

	struct st_cgi_rsp
	{
		0 require int ret;
		1 require string msg;
		2 require string out_json;
	};

	interface HttpCGI
	{
		int get(st_cgi_req cgi_req, out st_cgi_rsp rsp);
	};
};

