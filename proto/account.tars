#include "common.tars"

module Account
{
	enum EN_USER_TYPE
	{
		USER_NONE	= 0,
		USER_WX		= 1,
		USER_ALIPAY	= 2,
		USER_UID	= 100,
	};

	struct st_user_info
	{
		0 require int user_type; // EN_USER_TYPE
		1 require string user_id;
		2 require long uid;
		3 require string appid;
		4 require string access_token;
		5 optional CommonDef::st_page_context page_context;
	};
};
